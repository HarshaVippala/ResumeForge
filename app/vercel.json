{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "api/email.ts": {
      "maxDuration": 30
    },
    "api/tracking/process-unprocessed.ts": {
      "maxDuration": 30
    },
    "api/**/*.ts": {
      "maxDuration": 10
    },
    "src/app/api/email/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 10
    }
  }
}